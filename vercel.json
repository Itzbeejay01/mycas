
{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "api/**/*.css": {
      "runtime": "vercel-php@0.6.0"
    }
    },
  "routes": [
    { "src": "/(.*)",  "dest": "/api/index.php" },
    { "src": "/(.*)",  "dest": "/api/input.css" },
     { "src": "input.css", "dest": "input.css" }
   ]
}
