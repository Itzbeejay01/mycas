
{
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    },
    "api/**/*.css": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    { "src": "/(.*)",  "dest": "/api/index.php" },
     { "src": "/output.css", "dest": "/api/output.css" },
      { "src": "/script.js", "dest": "/api/script.js" }]
}
